{
  "name": "@layers/models",
  "version": "0.1.0",
  "description": "AI model registry with capabilities and pricing for Layers platform",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --exclude '**/integration/**'",
    "test:integration": "vitest run --include '**/integration/**'"
  },
  "devDependencies": {
    "@ai-sdk/black-forest-labs": "^1.0.7",
    "@ai-sdk/google": "^3.0.7",
    "ai": "^6.0.31",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "zod": "^4.3.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT"
}
